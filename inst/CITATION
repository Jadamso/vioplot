citHeader("To cite the enhanced vioplotx package in publications use:")

citEntry(entry = "Manual",
  title        = "vioplotx: Enhanced Violin Plot",
  author       = personList(as.person("S. Thomas Kelly")),
  year         = "2005",
  note         = "R package version 0.0.0.9",
  url          = "https://github.com/TomKellyGenetics/vioplotx",

  textVersion  =
  paste("S. Thomas Kelly (2016). vioplotx: Enhanced Violin Plot. R package version 0.0.0.9",
         "https://github.com/TomKellyGenetics/vioplotx")
)

citFooter(paste("Please also acknowledge the original package: \n citation(", "vioplot", ")", sep="\""))
